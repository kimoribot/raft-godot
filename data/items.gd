func get_item(id: String) -> Dictionary:
	return items.get(id, {})

func get_by_category(cat: Category) -> Array:
	var result = []
	for id in items:
		if items[id].get("category") == cat:
			result.append({"id": id, "data": items[id]})
	return result

func get_by_rarity(rarity: Rarity) -> Array:
	var result = []
	for id in items:
		if items[id].get("rarity") == rarity:
			result.append({"id": id, "data": items[id]})
	return result

func get_item_count() -> int:
	return items.size()
